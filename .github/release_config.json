{
    "categories": [
        {
            "title": "#### Bug Fixes",
            "labels": ["bug"]
        },
       {
            "title": "#### New Features",
            "labels": ["feature", "enhancement", "task"]
        },
        {
            "title": "#### Accessibility",
            "labels": ["accessibility"]
        },
        {
            "title": "#### Text Changes",
            "labels": ["text-change"]
        },
        {
            "title": "#### Community",
            "labels": ["community"]
        },            
        {
            "title": "#### Others / Chore",
            "labels": ["chore"]
        }                  
    ],
    "sort": "ASC",
    "template": "${{CHANGELOG}}",
    "pr_template": "- ${{TITLE}} #${{NUMBER}}",
    "empty_template": "- no changes",
    "max_tags_to_fetch": 200,
    "max_pull_requests": 200,
    "max_back_track_time_days": 90,
    "ignore_labels": [
      "admin"
    ],
    "base_branches": [
      "release/*"
    ]
}
